<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.demo.luocj.com.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.XRecyclerViewActivity" />
        <activity android:name=".activity.ButtonDialogActivity" />
        <activity android:name=".activity.ComplexRecyclerView" />
        <activity android:name=".adapter.TestActivity" />
        <activity android:name=".activity.OkGOActivity" />
        <activity android:name=".activity.RecyclerviewActivity" />
    </application>

</manifest>